<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Login.Getdata">

  <EditText
      android:id="@+id/nameEditText"
      android:layout_width="wrap_content"
      android:layout_height="48dp"
      android:layout_marginTop="232dp"
      android:ems="10"
      android:hint="Enter your name"
      android:inputType="textPersonName"
      android:textColor="@color/black"
      android:textColorHint="#565656"
      android:textStyle="italic"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.497"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <EditText
      android:id="@+id/addressEditText"
      android:layout_width="287dp"
      android:layout_height="0dp"
      android:layout_marginTop="400dp"
      android:layout_marginBottom="41dp"
      android:ems="10"
      android:hint="Enter your address"
      android:inputType="textPersonName"
      android:textColor="@color/black"
      android:textColorHint="#565656"
      android:textStyle="italic"
      app:layout_constraintBottom_toTopOf="@+id/roomEditText"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      tools:ignore="UnknownId" />

  <EditText
      android:id="@+id/parentEditText"
      android:layout_width="217dp"
      android:layout_height="64dp"
      android:layout_marginTop="312dp"
      android:ems="10"
      android:hint="Enter your parent's phone no."
      android:inputType="textPersonName"
      android:textColor="@color/black"
      android:textColorHint="#565656"
      android:textStyle="italic"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <EditText
      android:id="@+id/rollnoEditText"
      android:layout_width="wrap_content"
      android:layout_height="48dp"
      android:layout_marginTop="148dp"
      android:ems="10"
      android:hint="Enter your roll no."
      android:inputType="textPersonName"
      android:textColor="@color/black"
      android:textColorHint="#565656"
      android:textStyle="italic"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.497"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <Button
      android:id="@+id/saveDataButton"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="608dp"
      android:onClick="saveData"
      android:text="save data"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <EditText
      android:id="@+id/hostelEditText"
      android:layout_width="wrap_content"
      android:layout_height="48dp"
      android:layout_marginTop="76dp"
      android:ems="10"
      android:hint="Hostel"
      android:inputType="textPersonName"
      android:textColorHint="#565656"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.497"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <EditText
      android:id="@+id/roomEditText"
      android:layout_width="107dp"
      android:layout_height="48dp"
      android:layout_marginStart="40dp"
      android:layout_marginBottom="166dp"
      android:ems="10"
      android:hint="Room"
      android:inputType="textPersonName"
      android:textColorHint="#919191"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/phoneEditText"
      app:layout_constraintVertical_bias="0.943" />

  <EditText
      android:id="@+id/phoneEditText"
      android:layout_width="wrap_content"
      android:layout_height="48dp"
      android:layout_marginStart="32dp"
      android:layout_marginBottom="458dp"
      android:ems="10"
      android:hint="Phone Number"
      android:inputType="textPersonName"
      android:textColorHint="#919191"
      app:layout_constraintBottom_toTopOf="@+id/roomEditText"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>